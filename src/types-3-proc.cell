                    tables:                   [TableVar -> Table],
                    foreign_keys:             [BlockForeignKey],
                    nested_blocks:            [NestedDBVar -> DBSymbol],
                    init_code:                Instr*,
                    cleanup_code:             Instr*,
                    copy_state_code:          Instr*,
                    set_state_code:           Instr*,
