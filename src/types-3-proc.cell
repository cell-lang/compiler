                      set_at(var: WriteObjVar, idx: IntExpr, value: AtomicExpr),  // Ownership transfer: <value>

                      alloc_array(var: ArrayVar, size: Int32Expr),
                      write_array(var: ArrayVar, idx: Int32Expr, value: AnyTypeExpr),

                      write_rec_field(var: LocOptTagRecVar, label: Symbol, value: AnyTypeExpr, optional: Bool),
